{
  "name": "docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --fix",
    "format": "prettier --write .",
    "build:lib": "ng build angular-material-palette-generator",
    "publish:lib": "cd dist/angular-material-palette-generator && npm publish && cd -",
    "predocs": "rm -rf docs",
    "docs": "ng build --base-href='/angular-material-palette-generator/'",
    "postdocs": "mv dist/docs/browser docs && cp docs/index.html docs/404.html"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.1",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@fontsource/roboto": "^5.2.5",
    "@fontsource/roboto-mono": "^5.2.5",
    "@material-symbols/font-600": "^0.31.4",
    "@types/js-cookie": "^3.0.6",
    "colorjs.io": "^0.5.2",
    "js-cookie": "^3.0.5",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "wcag-contrast-utils": "^1.0.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.8",
    "angular-eslint": "19.6.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jasmine-core": "~5.7.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.33.0"
  }
}